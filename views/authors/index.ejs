<h2>Search Author</h2>
<form action="/authors" method="get">
    <label for="author">Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type=""submit">Search</button>
</form>
<br>

<% authors.forEach(author => { %>
    <div><%= author.name.toLowerCase() %></div>
<% }) %>

